{
	"version": 2,
	"builds": [
		{ "src": "app.py", "use": "@liudonghua123/now-flask" }
	],
	"routes": [
		{ "src": "/backup", "dest": "app.py" },
		{ "src": "/download", "dest": "app.py" },
		{ "src": "/download_makedown", "dest": "app.py" },
		{ "src": "/index", "dest": "app.py" },
		{ "src": "/", "dest": "app.py" },
		{ "src": "/*", "dest": "app.py" }
	]
}
